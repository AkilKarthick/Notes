private static void MinMaxinArray(int[] arra){
  int small = arra[0];
  int large = arra[0];

  for(int i=0; i<arra.length; i++){
   if(arra[i] < small){
      smalll = arra[i];
   }
  if(arra[i] > large){
      large = arra[i];
  }
  System.out.println(""+small);
  }
}
